{
    "schemaVersion": 1,
    "id": "pentamana",
    "version": "${version}",
    "name": "Pentamana",
    "description": "Very customizable mana library handles storing, ticking, and displaying mana.",
    "authors": [
        {
            "name": "CookedSeafood",
            "contact": {
                "homepage": "https://github.com/CookedSeafood"
            }
        }
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/pentamana",
        "sources": "https://github.com/CookedSeafood/pentamana"
    },
    "license": "GNU-GPL-3.0",
    "icon": "assets/pentamana/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "net.hederamc.pentamana.Pentamana"
        ],
        "client": [
            "net.hederamc.pentamana.PentamanaClient"
        ]
    },
    "mixins": [
        "pentamana.mixins.json",
        {
            "config": "pentamana.client.mixins.json",
            "environment": "client"
        }
    ],
    "depends": {
        "fabricloader": ">=0.18.1",
        "minecraft": "~1.21.6",
        "java": ">=21",
        "fabric-api": "*",
        "generic-registry": ">=2.0.0",
        "candy-wrapper": ">=2.0.2",
        "general-custom-data": ">=1.0.1"
    },
    "suggests": {
        "another-mod": "*"
    },
    "custom": {
        "loom:injected_interfaces": {
            "net/minecraft/class_1309": [
                "net/hederamc/pentamana/api/LivingEntityApi"
            ],
            "net/minecraft/class_1893": [
                "net/hederamc/pentamana/api/EnchantmentsApi"
            ],
            "net/minecraft/class_3222": [
                "net/hederamc/pentamana/api/ServerPlayerEntityApi"
            ]
        }
    },
    "accessWidener": "pentamana.accesswidener"
}